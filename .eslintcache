      return {\n        ...state,\n        adding: true,\n      };\n    case NEWS_ADD_SUCCEED:\n      return {\n        ...state,\n        items: [...state.items, action.payload],\n        adding: false,\n      };\n    case NEWS_DELETE:\n      return {\n        ...state,\n        items: state.items.filter((item) => item.id !== action.payload),\n      };\n    default:\n      return {\n        ...state,\n      };\n  }\n}\n\n/** Thunks **/\nexport function loadNews() {\n  return (dispatch) => {\n    dispatch({ type: LOAD_STARTED });\n    get(\"news\").then((json) =>\n      dispatch({\n        type: LOAD_SUCCEED,\n        payload: json,\n      })\n    );\n  };\n}\n\nexport function addedNews(title, text) {\n  return (dispatch) => {\n    dispatch({ type: NEWS_ADD_STARTED });\n    const todayDate = dayjs().format(\"YYYY-MM-DD\");\n    post(\"news\", {\n      title,\n      text,\n      date: todayDate,\n      verified: false,\n    }).then((json) =>\n      dispatch({\n        type: NEWS_ADD_SUCCEED,\n        payload: json,\n      })\n    );\n  };\n}\n\nexport function newsApproved(id) {\n  return (dispatch) => {\n    patch(`news/${id}`, { verified: true }).then((json) =>\n      dispatch({\n        type: NEWS_APPROVE,\n        payload: json,\n      })\n    );\n  };\n}\n\nexport function newsDeleted(id) {\n  return (dispatch) => {\n    del(`news/${id}`).then(() =>\n      dispatch({\n        type: NEWS_DELETE,\n        payload: id,\n      })\n    );\n  };\n}\n","C:\\Users\\ADmin\\Desktop\\Tests\\news-test\\src\\redux\\ducks\\users.js",[],"C:\\Users\\ADmin\\Desktop\\Tests\\news-test\\src\\components\\header\\Header.js",[],"C:\\Users\\ADmin\\Desktop\\Tests\\news-test\\src\\components\\main\\Main.js",[],"C:\\Users\\ADmin\\Desktop\\Tests\\news-test\\src\\api\\api.js",[],"C:\\Users\\ADmin\\Desktop\\Tests\\news-test\\src\\components\\news\\News.js",[],"C:\\Users\\ADmin\\Desktop\\Tests\\news-test\\src\\redux\\ducks\\application.js",[],"C:\\Users\\ADmin\\Desktop\\Tests\\news-test\\src